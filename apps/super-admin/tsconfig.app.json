{
  "extends": "@tesseract/typescript-config/vite.json",
  "include": [
    "src"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@tesseract/typescript-config/*": [
        "../../packages/typescript-config/*"
      ],
      "@tesseract/ui/*": [
        "../../packages/ui/src/*"
      ],
      "@tesseract/utils/*": ["../../packages/utils/src/*"],
      "@tesseract/domain/*": ["../../packages/domain/src/*"],
      "@tesseract/theme/*": ["../../packages/theme/src/*"],
      "@tesseract/rbac/*": ["../../packages/rbac/src/*"]
    }
  },
}